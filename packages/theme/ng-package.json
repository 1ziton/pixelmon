{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/@pixelmon/theme",
  "deleteDestPath": true,
  "lib": {
    "flatModuleFile": "theme",
    "entryFile": "public_api.ts",
    "umdModuleIds": {
      "@angular/cdk/overlay": "ng.cdk.overlay",
      "@angular/cdk/portal": "ng.cdk.portal",
      "ng-zorro-antd/drawer": "ng-zorro-antd/drawer",
      "ng-zorro-antd/modal": "ng-zorro-antd/modal",
      "ng-zorro-antd/icon": "ng-zorro-antd/icon",
      "date-fns": "DateFns",
      "@pixelmon/acl": "pixelmon.acl",
      "@pixelmon/util": "pixelmon.util"
    }
  },
  "whitelistedNonPeerDependencies": [
    "tslib",
    "angular",
    "date-fns",
    "ng-zorro-antd",
    "@pixelmon/acl",
    "@pixelmon/util",
    "@ant-design/icons-angular"
  ]
}
